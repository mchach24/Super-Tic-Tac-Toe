{
  "name": "super-tic-tac-toe",
  "version": "1.0.0",
  "description": "Super Tic Tac Toe: a 9x9 revamp of Tic Tac Toe with more strategic gameplay",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mchach24/Super-Tic-Tac-Toe.git"
  },
  "author": "Matthew Hachten",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/mchach24/Super-Tic-Tac-Toe/issues"
  },
  "homepage": "https://github.com/mchach24/Super-Tic-Tac-Toe#readme",
  "main": "",
  "scripts": {
    "dev": "webpack-dev-server --config ./webpack.config.js --mode development",
    "serve": "node dist/server/server.js",
    "nodemon": "nodemon dist/server.js",
    "coverage": "SET NODE_ENV=test&& nyc mocha",
    "test": "concurrently -n 0,tslint,mocha \"SET NODE_ENV=test\" \"npm:tslint\" \"mocha test/ --recursive --exit\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build": "concurrently -n b:ts,b:sass \"tsc\" \"sass src/public/scss/ --output dist/public/css/\"",
    "watch": "concurrently -n w:ts,w:sass \"tsc -w\" \"sass -w src/public/scss/ --output dist/public/css/\""
  },
  "dependencies": {
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0",
    "socket.io": "^2.1.1",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.11",
    "@types/socket.io": "^1.4.40",
    "concurrently": "^3.6.0",
    "copy-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "nyc": "^13.1.0",
    "sass": "^1.15.3",
    "ts-node": "^7.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.10.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  }
}
