    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "sass": "^1.16.0",
    "shx": "^0.3.2",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.43",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-dev-server": "^3.2.1"
  }
}
