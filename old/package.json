{
  "name": "super-tic-tac-toe",
  "version": "0.3.0",
  "author": "Matthew Hachten",
  "description": "Super Tic Tac Toe: a 9x9 revamp of Tic Tac Toe with more strategic gameplay",
  "license": "MIT",
  "homepage": "https://github.com/mchach24/Super-Tic-Tac-Toe#readme",
  "main": "src/server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mchach24/Super-Tic-Tac-Toe.git"
  },
  "private": true,
  "bugs": {
    "url": "https://github.com/mchach24/Super-Tic-Tac-Toe/issues"
  },
  "directories": {
    "lib": "src/server",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "dev": "ts-node-dev src/server",
    "start": "ts-node src/server",
    "test": "npm run some-typescript-compile && npm run mocha",
    "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
    "webpack": "webpack-dev-server --open",
    "server": "nodemon src/server/index",
    "dev:old": "concurrently --kill-others-on-fail -n build,server \"npm:build -- --watch\" \"npm:nodemon\"",
    "build": "concurrently -n webpack,ts \"webpack\" \"tsc\"",
    "nodemon": "nodemon dist/backend/index.js --watch",
    "coverage": "set NODE_ENV=test&& nyc mocha",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test:all": "npm run tslint && cross-env NODE_ENV=test npm run mocha",
    "start:seed": "cross-env NODE_ENV=test ts-node --seed --files src/",
    "start-prod": "npm run build && concurrently -n server \"node dist/backend/index.js\""
  },
  "dependencies": {
    "@feathers-plus/test-utils": "^0.3.6",
    "@feathersjs/authentication": "^4.3.5",
    "@feathersjs/authentication-local": "^4.3.5",
    "@feathersjs/authentication-oauth": "^4.3.5",
    "@feathersjs/configuration": "^4.3.4",
    "@feathersjs/errors": "^4.3.4",
    "@feathersjs/express": "^4.3.5",
    "@feathersjs/feathers": "^4.3.4",
    "@feathersjs/socketio": "^4.3.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "feathers-hooks-common": "^4.20.0",
    "feathers-mongodb": "^5.0.1",
    "helmet": "^3.21.1",
    "lodash.merge": "^4.6.1",
    "mongodb": "^3.3.2",
    "mongodb-core": "^3.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^7.1.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.5.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^4.5.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/feathersjs__configuration": "^1.0.3",
    "@types/feathersjs__express": "^1.1.6",
    "@types/feathersjs__feathers": "^3.1.5",
    "@types/feathersjs__socketio": "^3.0.7",
    "@types/helmet": "0.0.40",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash.merge": "^4.6.4",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.1.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/request-promise": "^4.1.42",
    "@types/serve-favicon": "^2.2.31",
    "@types/socket.io": "^1.4.40",
    "@types/webpack": "^4.4.25",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-dev-server": "^3.1.2",
    "@types/winston": "^2.4.4",
    "axios": "^0.19.0",
    "concurrently": "^3.6.0",
    "copy-webpack-plugin": "^4.6.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.4.5",
    "mocha": "^6.2.1",
    "node-sass": "^4.11.0",
